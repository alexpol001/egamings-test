    "nest-api": {
      "root": "apps/nest-api",
      "sourceRoot": "apps/nest-api/src",
      "projectType": "application",
      "prefix": "backend",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/nest-api",
            "main": "apps/nest-api/src/main.ts",
            "tsConfig": "apps/nest-api/tsconfig.json",
            "assets": [
              {
                "input": "libs/shared/assets",
                "glob": "**/*",
                "output": "assets"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest-api/src/environments/environment.ts",
                  "with": "apps/nest-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nest-api:build"
          }
        },
        "build2": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "customWebpackConfig": {
              "path": "webpack-server.config.ts"
            },
            "outputPath": "dist/apps/nest-api",
            "main": "apps/nest-api/src/main.ts",
            "tsConfig": "apps/nest-api/tsconfig.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/nest-api/src/environments/environment.ts",
                  "with": "apps/nest-api/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve2": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ng-frontend:build",
            "serverTarget": "nest-api:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-frontend:build:production",
              "serverTarget": "nest-api:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/nest-api/tsconfig.app.json",
              "apps/nest-api/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/nest-api/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nest-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng-frontend",
      "sourceRoot": "apps/ng-frontend/src",
      "prefix": "egamings",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.ts",
              "mergeStrategies": {
                "externals": "replace"
              }
            },
            "allowedCommonJsDependencies": [
              "helpful-decorators",
              "lodash"
            ],
            "outputPath": "dist/apps/ng-frontend/browser",
            "index": "apps/ng-frontend/src/index.html",
            "main": "apps/ng-frontend/src/main.ts",
            "polyfills": "apps/ng-frontend/src/polyfills.ts",
            "tsConfig": "apps/ng-frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ng-frontend/src/favicon.ico",
              {
                "input": "libs/shared/assets",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "styles": [
              "libs/shared/styles/styles.scss",
              {
                "input": "libs/shared/styles/themes/light-theme.scss",
                "lazy": true,
                "bundleName": "light-theme"
              },
              {
                "input": "libs/shared/styles/themes/dark-theme.scss",
                "lazy": true,
                "bundleName": "dark-theme"
              }
            ],
            "extractCss": true,
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-frontend/src/environments/environment.ts",
                  "with": "apps/ng-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.ts",
              "mergeStrategies": {
                "externals": "replace"
              }
            },
            "browserTarget": "ng-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-frontend:build:production"
            }
          }
        },
        "server": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.ts"
            },
            "outputPath": "dist/apps/ng-frontend/server",
            "main": "apps/ng-frontend/server.ts",
            "tsConfig": "apps/ng-frontend/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "bundleDependencies": true,
              "fileReplacements": [
                {
                  "replace": "apps/ng-frontend/src/environments/environment.ts",
                  "with": "apps/ng-frontend/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "ssr-serve": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ng-frontend:build",
            "serverTarget": "ng-frontend:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-frontend:build:production",
              "serverTarget": "ng-frontend:server:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-frontend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-frontend/tsconfig.app.json",
              "apps/ng-frontend/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ng-frontend/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng-frontend/jest.config.js",
            "passWithNoTests": true
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "ng-frontend:build:production",
            "serverTarget": "nest-api:build:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
