  "scripts": {
    "ng": "nx",
    "postinstall": "ngcc",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clear": "killall node",
    "nest-ms-mongo:migration:generate": "cd apps/nest-ms-mongo/src/app/database npx typeorm migration:generate -n PostTable",
    "serve:env": "docker-compose up",
    "serve:client": "npx nx ssr-serve ng-frontend & npx nx serve ng-mobile --port=4300",
    "serve:server": "npx nx serve nest-ms-rabbit & npx nx serve nest-ms-redis & npx nx serve nest-ms-mongo & npx nx serve nest-api",
    "dev:ssr:frontend": "npx nx frontend:serve-ssr",
    "serve:ssr:frontend": "node dist/apps/frontend/server/main.js",
    "prebuild:ssr:frontend": "npm run postinstall",
    "build:ssr:frontend": "nx build frontend --prod && nx run frontend:server:production",
    "prerender:frontend": "nx frontend:prerender",
    "build.mobile": "nx build mobile",
    "prepare.mobile": "npm run clean.mobile && npm run build.mobile",
    "prepare.mobile.android": "npm run prepare.mobile && cd apps/mobile && npm run cap.add.android",
    "open.mobile.android": "cd apps/mobile && npm run cap.android",
    "sync.mobile": "cd apps/mobile && npm run cap.copy",
    "clean.mobile": "cd apps/mobile && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
  },
  "private": true,
